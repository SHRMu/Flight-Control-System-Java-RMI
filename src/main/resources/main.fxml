<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
   <children>
      <Button fx:id="deleteBtn" layoutX="379.0" layoutY="353.0" mnemonicParsing="false" onAction="#delete" prefHeight="25.0" prefWidth="72.0" text="Delete" />
      <Button fx:id="editBtn" layoutX="282.0" layoutY="353.0" mnemonicParsing="false" onAction="#edit" prefHeight="25.0" prefWidth="72.0" text="Edit" />
      <Button fx:id="addBtn" layoutX="184.0" layoutY="353.0" mnemonicParsing="false" onAction="#add" prefHeight="25.0" prefWidth="72.0" text="Add" />
      <TableView fx:id="flightTable" layoutY="-7.0" prefHeight="352.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="colOpAirline" prefWidth="100.0" text="Operating Airline" />
          <TableColumn fx:id="colIATACode" prefWidth="100.0" text="IATA Code" />
          <TableColumn fx:id="colFlightNum" prefWidth="100.0" text="Flight Number" />
          <TableColumn fx:id="colDepature" prefWidth="100.0" text="Depature" />
          <TableColumn fx:id="colArrival" prefWidth="100.0" text="Arrival" />
          <TableColumn fx:id="colDTerminal" prefWidth="100.0" text="DTerminal" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
